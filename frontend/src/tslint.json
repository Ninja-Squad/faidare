{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "faidare",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "faidare",
      "kebab-case"
    ],
    "template-cyclomatic-complexity": [
      true,
      12
    ]
  }
}
